doctype 5
html(lang="en-US")
    block vars
        - var app = 'default'

    head
        meta(charset="UTF-8")
        title Appular Project

        block head

    body(data-appular-app="#{app}")

        header 
            block header

        block content

        footer
            block footer

        block scripts
            - if (environment === 'development')
                script(src="/js/dev/libraries/require/config.js")
                script(src="/js/dev/libraries/require/require-2.1.10.js", data-main="initialize")
            - else
                script(src="/js/build/appular.js")